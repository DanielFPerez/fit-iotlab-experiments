    <plugin_config>
      <script>SINK_ID = 1;

sink_mote = mote.getSimulation().getMoteWithID(SINK_ID);
log.log("Sink Mote: " + sink_mote + "\n");

while (true) {
    YIELD();
    log.log(id + ": " + msg + "\n");
    if (id != SINK_ID) {
        write(sink_mote, msg);
    }
}</script>
